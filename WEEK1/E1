class Solution {
    public int climbStairs(int n) {
        if (n==0 || n==1 || n==2)
            return n;

        int first=1;
        int sec=2;
        int third=0;
        for (int i=0;i<n-2;i++)
        {
            third=first+sec;
            first=sec;
            sec=third;
        }
        return third;
        
    }
