class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int n=nums.length;
        int[] small=new int[n];
        for (int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                if(i!=j)
                {
                    if(nums[j]<nums[i])
                    {
                        small[i]+=1;
                    }
                }
            }
        }

        return small;
        
    }
}
